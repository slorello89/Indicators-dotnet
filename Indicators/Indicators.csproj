<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="data\AAPL.csv" />
    <None Remove="data\JPM.csv" />
    <None Remove="data\SPY.csv" />
    <None Remove="data\TSLA.csv" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Daany.DataFrame" Version="0.6.4" />
    <PackageReference Include="Numpy" Version="3.7.1.21" />
    <PackageReference Include="ScottPlot" Version="4.0.38" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="data\AAPL.csv">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="data\JPM.csv">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="data\SPY.csv">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="data\TSLA.csv">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
